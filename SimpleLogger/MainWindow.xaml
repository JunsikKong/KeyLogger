            <Border Name="BorderNumReturn"  Grid.Column="45" Grid.ColumnSpan="2" Grid.Row="6" Grid.RowSpan="2" Style="{StaticResource BorderKey}" >
                <TextBlock Text="Enter" Style="{StaticResource TextBlockKey}" />
            </Border>
            <Border Name="BorderNum0"  Grid.Column="39" Grid.ColumnSpan="4" Grid.Row="7" Grid.RowSpan="1" Style="{StaticResource BorderKey}" >
                <TextBlock Text="0" Style="{StaticResource TextBlockKey}" />
            </Border>
            <Border Name="BorderNumPeriod"  Grid.Column="43" Grid.ColumnSpan="2" Grid.Row="7" Grid.RowSpan="1" Style="{StaticResource BorderKey}" >
                <TextBlock Text="." Style="{StaticResource TextBlockKey}" />
            </Border>


        </Grid>
        
        
        <Border Grid.Column="1" Grid.RowSpan="2" Background="DarkGray"/>
        <Button Grid.Column="2" Name="btnStart" Content="Start" Click="btnStart_Click" />
        <Button Grid.Column="3" Name="btnStop" Content="Stop" Click="btnStop_Click" />
        <Button Grid.Column="4" Name="btnPause" Content="Pause" Click="btnPause_Click" />
        <TextBox Grid.Column="2" Grid.ColumnSpan="3" Grid.Row="1" Name="tbxLog" VerticalScrollBarVisibility="Visible"/>
    </Grid>
</mah:MetroWindow>